{
  "name": "jugar-ai-render",
  "version": "2.0.0",
  "description": "Backend de IA avançado para JugarEnChile.com con integración Tawk.to, RAG, OpenAI, PostgreSQL, Redis y Analytics",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "generate-embeddings": "node scripts/generate-embeddings.js",
    "test": "node --test src/tests/**/*.test.js",
    "test:watch": "node --test --watch src/tests/**/*.test.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:seed": "node scripts/seed-database.js",
    "analytics:report": "node scripts/generate-analytics-report.js",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "chatbot",
    "tawk.to",
    "openai",
    "rag",
    "embeddings",
    "postgresql",
    "redis",
    "analytics"
  ],
  "author": "raildozouk",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "axios": "^1.6.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
